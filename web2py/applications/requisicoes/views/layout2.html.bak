<script>
$.fn.fileinputBsVersion = '5.1.1'; // for example if using bootstrap css 3.4.1 version
var hided = 0
function hide1(){
    if (hided==0){
        $(".conteudo").css("left","-290px").css("width","97%");
        $(".sidebar").css("left","-300px");
        $(".b-example-divider").css("left","-300px");
        hided = 1
    }else{
        $(".conteudo").css("left","unset").css("width","calc(97% - 300px)");
        $(".sidebar").css("left","unset");
        $(".b-example-divider").css("left","300px");
        hided = 0
    }
}
function hide(){
    if (hided==0){
        $(".conteudo").css("left","-290px").css("width","97%");
        $(".sidebar").css("left","-300px");
        $(".b-example-divider").css("left","-25px");
        hided = 1
    }else{
        $(".conteudo").css("left","0px").css("width","calc(97% - 300px)");
        $(".sidebar").css("left","0px");
        $(".b-example-divider").css("left","300px");
        hided = 0
    }
}
$.fn.fileinputLocales['pt-BR'] = {
