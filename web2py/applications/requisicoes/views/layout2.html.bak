/* Alertas*/
.w2p_flash,.w2p_flash2,.w2p_flash21,.w2p_flash3,.w2p_flash4,.w2p_flash5 {
    position: fixed;
    width: 25%;
    top: 10px;
    right: 1%;
    cursor: default;
    text-align: center;
    z-index: 5620;
}

.marcaDagua {
    font-family: arial;
    color: #FF0000;
    font-size: 4em;
    right: 1em;
    opacity: 0.2;
    position: fixed;
    text-align: center;
    top: 3.3em;
    transform: rotate(-35deg);
    visibility: visible;
    border: 1px;
    border-style: dashed;
    border-width: thick;
}
/*  Reedição do bootstrap 3    */
.popover{
    max-width:500px;
}

.help-block{
    font-size: 1rem;
    font-weight: 300;
    line-height: 1.0;
    color: red;
    text-align: left;
    display: none;
}
._container {
    display: grid;
    position: relative;
    padding-left: 35px;
    margin-bottom: 12px;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}
._container input {
    position: absolute;
    opacity: 0;
    cursor: pointer;
}
._container input:checked ~ .checkmark {
    background-color: #2196F3;
}
.checkmark {
    position: absolute;
    top: 0;
    left: 0;
    height: 21px;
    width: 21px;
    background-color: #eee;
    border-radius: 50%;
    margin-top: 4px;
}
._container input:checked ~ .checkmark::after {
    display: block;
}
._container .checkmark::after {
    top: 7px;
    left: 7px;
    width: 7px;
    height: 7px;
    border-radius: 50%;
    background: white;
}
.checkmark::after {
    content: "";
    position: absolute;
    display: none;
}

.nav-item .nav-link.active{
    color: #fff;
    background-color: #0d6efd;
}
.sidebar{
    width: 300px;
    position: fixed;
    top: 0;
