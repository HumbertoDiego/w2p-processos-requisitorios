function replace(el){
    var popo = el.parentElement.parentElement.parentElement
    //console.log(popo);
    var txtareaval = popo.children[0].children[1].value
    if (txtareaval.trim().length<1){
        txtareaval = "_"
    }
    var modo = "gerente";
    var lista_modos = ["gerente","carona","dispensa","inex","anul"];
    for (let i=0;i<lista_modos.length;i++){
        //console.log(lista_modos[i]);
        if(popo.id.includes(lista_modos[i])){
            console.log(popo.id);
            modo = lista_modos[i];
        }
    }
    var campo = popo.id.replace("_popover","").replace("-anul","").replace("-inex","").replace("-dispensa","").replace("-carona","").replace("-gerente","");
    var search = "span[id^='"+campo+"']";

    // Cantinho para ajax post request
    var params = 'edit=1&processo={{=secao+"_"+ano+"_"+nr}}&modo='+modo+'&campo='+campo+'&valor='+txtareaval
    var oReq = new XMLHttpRequest();
    oReq.onreadystatechange = function() {
        if (oReq.readyState === 4) {
            if (oReq.status == 200){
                console.log(oReq.response);
                var obj = JSON.parse(oReq.response)
                if ("redirect2" in obj){
                    window.location.href = obj['redirect2'];
                }
                $(search).each(function(index,element){
                    element.innerText = txtareaval;
                    element.classList.add("edited");
                });
                $('.header').each(function(){
                    $( this ).html(obj['hashed']);
