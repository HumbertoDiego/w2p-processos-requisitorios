                                .replace(/\\b/g, "\\b")
                                .replace(/\\f/g, "\\f")
                                .replace(/\\ /g, " ")
                                .replace(/\t/g,  '');
            console.log(dadosjson);
            var objdados_embaralhados = JSON.parse(dadosjson);
        }
        let objdados = {};
        Object.keys(objdados_embaralhados).sort().map(i=>objdados[i]=objdados_embaralhados[i]);
        console.log(objdados);
        function pop(dados){
            // Populando as datas a partir de dados["data"]->dd/mm/yyyy
            $("input[id^=data]").val(dados["data"]);
            $("[id^=datepicker]").attr("data-date",dados["data"]);
            const meses = ["Jan", "Fev", "Mar", "Abr", "Mai", "Jun", "Jul", "Ago", "Set", "Out", "Nov", "Dez"];
            const meses_completos = ["Janeiro", "Feveveiro", "Março", "Abril", "Maio", "Junho", "Julho", "Agosto", "Setembro", "Outubro", "Novembro", "Dezembro"];
            $("span[id^=dataextenso]").html(dados["data"].split("/")[0]+" de "+meses_completos[parseInt(dados["data"].split("/")[1])-1]).addClass("edited");
            $("span[id^=dataresumidalower]").html(dados["data"].split("/")[0]+" "+meses[parseInt(dados["data"].split("/")[1])-1]);
            $("span[id^=dataresumidaupper]").html(dados["data"].split("/")[0]+" "+meses[parseInt(dados["data"].split("/")[1])-1].toUpperCase());
            if (dados["data_edited"]){$("span[id^=dataextenso]").addClass("edited");}

            // Populando os demais campos
            for (const [key, value] of Object.entries(dados)) {
              if (!(key.includes("edited")) && !(key.includes("data"))){
                  // Tem itens que são de listas e se houver mais que um deve-se criar as linhas respectivas e populá-las
                  if (parseInt(key.split("-")[1])>0){
                      while($("[id^="+key+"]").length == 0){
                          //console.log(key);
                          document.getElementById("btnaddline-gerente").click();
                      }
                  }
                  //$("span[id^="+key+"]").html(value.replaceAll("\n","</br>"));
                  $("span[id^="+key+"]").html(value.replace(/\n/g,"</br>"));
                  $("input[id^="+key+"]").val(value);
                  $("span[id^="+key+"upper]").html(value.replace(/\n/g,"</br>").toUpperCase());
                  if (dados[key+"_edited"]){$("span[id^="+key+"]").addClass("edited");}
              }

              // Populando as assinaturas
              if (!(key.includes("edited")) && (key.includes("ass_"))){
                  contem_assinaturas = true;
                  var ass_div = document.getElementById(key);
                  if (ass_div!= null){
                      ass_div.lastElementChild.style = "display:none";
                      ass_div.firstElementChild.firstElementChild.innerHTML=value;
                      ass_div.firstElementChild.style="display:block";
                  }
              }

            }
            if (dados["modo"]){
                var lista_modos = ["gerente","carona","dispensa","inex","anul"];
                var modos_rejeitados = lista_modos.filter(function(v,i,a){return v!=dados["modo"]})
                var el = document.querySelector("a[href='#tab_"+dados["modo"]+"']");
                el.classList.add("active");
                el = document.querySelector("div[id='tab_"+dados["modo"]+"']");
                el.classList.add("active");
                var els;
                for (let i=0; i<modos_rejeitados.length;i++){
                    els = document.querySelector("a[href='#tab_"+modos_rejeitados[i]+"']");
                    els.style.color="#9fa8af";
                    els.classList.remove("active");

                    els = document.querySelector("div[id='tab_"+modos_rejeitados[i]+"']");
                    els.classList.remove("active");
                }
            }
        }
        pop(objdados);
    }
