function multiplicar(el){
    var popo = el.parentElement.parentElement.parentElement;
    var campo = popo.id.replace("_popover","").replace("-inex","").replace("-dispensa","").replace("-carona","").replace("-gerente","");
    var qtitens = parseFloat(document.getElementById("qtitens-"+campo.split("-")[1]+"-gerente").innerText.replaceAll(".","").replaceAll(",","."));
    var valor = parseFloat(document.getElementById("valor-"+campo.split("-")[1]+"-gerente").innerText.replaceAll(".","").replaceAll(",","."));
    //var qtitens = parseFloat(document.getElementById("qtitens-"+campo.split("-")[1]+"-gerente").innerText.replace(/./g,"").replace(/,/g,"."));
    //var valor = parseFloat(document.getElementById("valor-"+campo.split("-")[1]+"-gerente").innerText.replace(/./g,"").replace(/,/g,"."));
    //console.log(campo);//somaparcial-0
    console.log(qtitens);console.log(valor);
    //qtitens-0-dispensa
    //valor-0-dispensa
    if(isNaN(qtitens) || isNaN(valor)){
        popo.children[0].children[1].value = "NaN";
    }else{
        popo.children[0].children[1].value = (qtitens*valor).toLocaleString('pt-BR',{ minimumFractionDigits: 2, maximumFractionDigits: 2});
        popo.firstElementChild.lastElementChild.lastElementChild.click();
    }
}
function somar(el){
    var popo = el.parentElement.parentElement.parentElement;
    var campo = document.getElementById(popo.id.replace("_popover",""));
    var table = campo.parentElement.parentElement.parentElement.parentElement;//table
    var somasparciais = table.querySelectorAll('[id^="somaparcial"]');
    var somatotal = 0;
    somasparciais.forEach(function(e,i){
        if (/popover/g.test(e.id)){
            //popovers não
        }else{
            somatotal+=parseFloat(e.innerText.replaceAll(".","").replaceAll(",","."));
        }
    });
    if(isNaN(somatotal)){
        popo.children[0].children[1].value = "NaN";
    }else{
        popo.children[0].children[1].value = somatotal.toLocaleString('pt-BR',{ minimumFractionDigits: 2, maximumFractionDigits: 2});
        popo.firstElementChild.lastElementChild.lastElementChild.click();
    }
}
