{{block sidebar}}
<span class="d-flex align-items-center flex-shrink-0 p-3 link-dark text-decoration-none border-bottom">
        <form action="{{=URL('default','index')}}" >
            <div class="row">
                <span class="fs-5 fw-semibold col-4">Seção:</span>
                <div class="col-7" style="padding:0px">
                    <select name="sec" onchange="ajax_getanos()" class="form-select">
                        {{for s in SECOES:}}
                        <option value="{{=s}}" {{="selected" if secao==s else ""}}>{{=s}}</option>
                        {{pass}}
                    </select>
                </div>
            </div>
            <div class="row">
                <span class="fs-5 fw-semibold col-4">Ano:</span>
                <input class="col-7" id="ano" name="ano" type="text" value="{{=request.vars.ano}}" placeholder="Selecione" style="height: calc(2.25rem + 4px);border: 1px solid #ced4da;" >
               <!--  <input list="datalistOptions"> obsoleto após a adição do datepicker -->
                <datalist id="datalistOptions">
                  <option value="2021">
                  <option value="2022">
                  <option value="2023">
                  <option value="2024">
                </datalist>
                <!-- obsoleto após a adição do datepicker -->
                <input type="hidden" id="secao_change" name="secao_change" value="y" style="width:100%">
            </div>
            <div class="row">
                <input type="submit" value="OK" class="btn btn-primary col-7 offset-md-4">
            </div>
        </form>
</span>
    <div class="list-group list-group-flush border-bottom scrollarea">
        {{if editavel:}}
      <a href="" class="list-group-item list-group-item-action active py-3 lh-tight" aria-current="true" data-bs-toggle="modal" data-bs-target="#myModal5">
        <div class="d-flex w-100 align-items-center justify-content-between">
          <strong class="mb-1">Novo Processo</strong>
            <input type="hidden" id="secao_ori" value="{{=request.vars.secao+'/'+request.vars.ano}}">
          <small class="badge bg-success">+</small>
        </div>
      </a>
        {{pass}}
      {{for pr in rows:}}
        {{try:}}
            <a href="{{=URL('default','index',vars=dict(secao=pr.secao_ano_nr.split('_')[0],ano=pr.secao_ano_nr.split('_')[1],nr=pr.secao_ano_nr.split('_')[2]))}}" class="list-group-item list-group-item-action py-3 lh-tight {{if request.vars.nr==pr.secao_ano_nr.split('_')[2]:}} azul{{pass}}">
            <div class="d-flex w-100 align-items-center justify-content-between">
              <div class="col-10 mb-1 small max60chars" title="{{=pr.resumo}}">{{=pr.resumo}}</div>
              <small class="badge {{='bg-success' if pr.valido==True else 'bg-danger' if pr.valido==False else 'bg-transparente text-dark'}}">{{="%02d"%int(pr.secao_ano_nr.split('_')[2])}}</small>
            </div>
          </a>
        {{except:}}
            {{conitnue}}
        {{pass}}
      {{pass}}
    </div>
{{end}}
